#include <stdio.h>

// 定义枚举类型
typedef enum 
{
    GPIO_Speed_2MHz,
    GPIO_Speed_10MHz, 
    GPIO_Speed_50MHz
} 
GPIO_Speed_Type;

// 定义结构体
typedef struct 
{
    GPIO_Speed_Type GPIO_Speed;
} 
GPIO_InitTypeDef;

// GPIO结构体初始化函数
void GPIO_StructureInit(GPIO_InitTypeDef *GPIO_InitStruct) 
{
    GPIO_InitStruct->GPIO_Speed = GPIO_Speed_2MHz;
}

int main() 
{
    GPIO_InitTypeDef GPIO_InitStruct;
    
    // 调用初始化函数
    GPIO_StructureInit(&GPIO_InitStruct);
    
    // 验证初始化结果
    printf("GPIO Speed initialized to: %d\n", GPIO_InitStruct.GPIO_Speed);
    
    return 0;
}
